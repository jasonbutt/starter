    "axios": "^0.21.1",
    "bcrypt": "^5.0.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-csp": "^0.1.3",
    "express-mongo-sanitize": "^2.0.2",
    "express-rate-limit": "^5.2.3",
    "helmet": "^4.4.1",
    "hpp": "^0.2.3",
    "html-to-text": "^7.0.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.11.13",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "nodemailer": "^6.4.17",
